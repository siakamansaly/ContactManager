<!DOCTYPE html>
<html lang="en">

{{ include('core/head.html.twig') }}

<body id="page-top">

    <!-- Page Wrapper -->
    <div id="wrapper">

        {{ include('core/sidebar.html.twig') }}

        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">

                {% if app.user %}
                {{ include('core/navbar.html.twig') }}
                {% endif %}
                

                <!-- Begin Page Content -->
                <div class="container-fluid">

                    {# include('core/exemple.html.twig') #}
                    {% block body %}{% endblock %}

                </div>
                <!-- /.container-fluid -->

            </div>
            <!-- End of Main Content -->

            {{ include('core/footer.html.twig') }}

        </div>
        <!-- End of Content Wrapper -->

    </div>
    <!-- End of Page Wrapper -->

    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>

    {{ include('core/modal.html.twig') }}


</body>

</html>